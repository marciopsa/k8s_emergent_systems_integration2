data Field {
	Type type
	char name[]
const int FLAG_RECURSION = 0x1
	byte flags
	}

data Type {
const byte INTEGER		= 1
const byte DECIMAL		= 2
const byte DATA			= 3
const byte OBJECT		= 4
const byte ARRAY		= 5
const byte THREAD		= 6
const byte FUNCTION		= 7
const byte EVENT		= 8
const byte COMPONENT	= 9
	byte class
const byte F_BOOL		= 0x1
const byte F_CHAR		= 0x2
	byte flags
	int size
	Field fields[]
	}